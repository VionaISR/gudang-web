<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="UTF-8" />
  <title>Stock Barang</title>
  <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.0/dist/css/bootstrap.min.css" rel="stylesheet">
</head>
<body class="container">
  <h1 class="mt-4 mb-4">Stock Barang</h1>
  <table class="table table-bordered">
    <thead class="table-dark">
      <tr>
        <th>ID</th>
        <th>Nama</th>
        <th>Brand</th>
        <th>Daya</th>
        <th>Frekuensi</th>
        <th>RPM</th>
        <th>Penggerak</th>
        <th>Transmisi</th>
      </tr>
    </thead>
    <tbody>
      <% barang.forEach(item => { %>
        <tr>
          <td><%= item.id %></td>
          <td><%= item.nama %></td>
          <td><%= item.brand %></td>
          <td><%= item.daya %></td>
          <td><%= item.frekuensi %></td>
          <td><%= item.rpm %></td>
          <td><%= item.penggerak %></td>
          <td><%= item.transmisi %></td>
        </tr>
      <% }) %>
    </tbody>
  </table>
</body>
</html>
